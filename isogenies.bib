@InProceedings{8023082,
  author    = {Bos, Joppe W. and Friedberger, Simon},
  title     = {Fast Arithmetic Modulo $2^x p^y \pm 1$},
  booktitle = {2017 IEEE 24th Symposium on Computer Arithmetic (ARITH)},
  year      = {2017},
  month     = jul,
  pages     = {148--155},
  doi       = {10.1109/ARITH.2017.15},
  abstract  = {We give a systematic overview of techniques to compute arithmetic modulo 2xpy± 1 and propose improvements. This is useful for computations in the supersingular isogeny Diffie-Hellman (SIDH) key-exchange protocol which is one of the more recent contenders in the post-quantum public-key arena. One of the main computational bottlenecks in this cryptographic key-exchange protocol is computing modular arithmetic in a finite field defined by a prime of this special shape. Recent implementations already use this special prime shape to speed up the cryptographic implementations but it remains unclear if the choices made are optimal or if one can do better. Our overview shows that in the SIDH setting, where arithmetic over a quadratic extension field is required, the approaches based on Montgomery multiplication are to be preferred. Based on our results, we give selection criteria for such moduli and the outcome of our search reveals that there exist moduli which result in even faster implementations.},
  groups    = {Isogenies},
  issn      = {1063-6889},
  keywords  = {cryptographic protocols;public key cryptography;quantum cryptography;fast arithmetic modulo;supersingular isogeny Diffie-Hellman key-exchange protocol;SIDH key-exchange protocol;post-quantum public-key arena;computational bottlenecks;cryptographic key-exchange protocol;modular arithmetic;finite field;Montgomery multiplication;selection criteria;quadratic extension field;Protocols;Shape;Public key cryptography;Elliptic curves;Optimization;Electronic mail;montgomery multiplication;barrett reduction;post-quantum cryptography;sidh},
}

